{
    "compilerOptions": {
        "outDir": "./build",
        "moduleResolution": "node",
        "module": "es6",
        "target": "es5",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "./src/*"
            ],
        }
    },
    "include": [
        "./images.d.ts",
        "./src/**/*"
    ],
    "exclude": [
        "node_modules",
    ]
}